  "scripts": {
    "start": "npm run dev",
    "dev": "nodemon bin/www",
    "staging": "node bin/production.www",
    "production": "npm run sass && npm run -s build && webpack -p --config ./webpack.production.config.js && node bin/production.www",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel server -D -d dist --presets es2015,stage-2",
    "test": "jest --watch",
    "test-server": "NODE_ENV=test npm run dev",
    "sass": "sass client/styles/base.scss:server/public/styles/main.min.css",
    "sassWatch": "sass --watch client/styles/main.scss:server/public/styles/main.min.css",
    "lintServer": "esw -w server test",
    "lintClient": "esw -w \"client/**/*.jsx\" \"client/**/*.js\" test",
    "lintFixServer": "eslint server/*.js --fix || true",
    "lintFixClient": "eslint client/**/*.jsx --fix && eslint client/**/*js --fix || true"
  },
